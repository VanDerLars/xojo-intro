#tag WindowCode
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  #Pragma Unused X
		  #Pragma Unused Y
		  
		  RaiseEvent clicked
		End Function
	#tag EndEvent

	#tag Event
		Sub Open()
		  Self.lblTitle.Text = Self.title
		  self.tbMessage.Text = self.message
		End Sub
	#tag EndEvent


	#tag Hook, Flags = &h0
		Event clicked()
	#tag EndHook


#tag EndWindowCode

